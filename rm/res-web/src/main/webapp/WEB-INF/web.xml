<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListContainers</servlet-name>
  	<display-name>ListContainers</display-name>
  	<description></description>
  	<servlet-class>com.mes.c3rm.web.servlets.ListContainers</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContainerActions</servlet-name>
  	<display-name>ContainerActions</display-name>
  	<description></description>
  	<servlet-class>com.mes.c3rm.web.servlets.ContainerActions</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ContainerActions</servlet-name>
  	<url-pattern>/res-web</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>res-web</welcome-file>
  </welcome-file-list>
</web-app>
